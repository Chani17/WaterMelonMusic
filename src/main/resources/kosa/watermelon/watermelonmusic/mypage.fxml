		prefHeight="341.0" prefWidth="600.0">
		<columns>
			<TableColumn fx:id="ranking"
				prefWidth="42.333343505859375" text="순위" />
			<TableColumn fx:id="songName" prefWidth="194.0"
				text="곡 정보" />
			<TableColumn fx:id="artistName" prefWidth="186.0"
				text="아티스트" />
			<TableColumn prefWidth="57.33331298828125" text="듣기" />
