	<Label layoutX="19.0" layoutY="21.0" prefHeight="22.0"
		prefWidth="150.0" text="▶ 마이페이지">
		<font>
			<Font name="D2Coding" size="20.0" />
		</font>
	</Label>
	<ImageView fx:id="profile_Image" fitHeight="160.0"
		fitWidth="160.0" layoutX="14.0" layoutY="85.0" pickOnBounds="true"
		preserveRatio="true" />
	<Label layoutX="12.0" layoutY="296.0" prefHeight="22.0"
		prefWidth="50.0" text="닉네임">
		<font>
			<Font name="D2Coding Bold" size="12.0" />
		</font>
	</Label>
	<TextField fx:id="userNAME_TextField" layoutX="55.0"
		layoutY="296.0" prefHeight="22.0" prefWidth="118.0">
		<font>
			<Font name="D2Coding" size="12.0" />
		</font>
	</TextField>

	<Label layoutX="12.0" layoutY="329.0" prefHeight="22.0"
		prefWidth="50.0" text="아이디">
		<font>
			<Font name="D2Coding Bold" size="12.0" />
		</font>
	</Label>
	<TextField fx:id="userID_TextField" layoutX="55.0"
		layoutY="329.0" prefHeight="22.0" prefWidth="118.0">
		<font>
			<Font name="D2Coding" size="12.0" />
		</font>
	</TextField>

	<Label layoutX="11.0" layoutY="361.0" prefHeight="22.0"
		prefWidth="50.0" text="이메일">
		<font>
			<Font name="D2Coding Bold" size="12.0" />
		</font>
	</Label>
	<TextField fx:id="userEMAIL_TextField" layoutX="55.0"
		layoutY="361.0" prefHeight="22.0" prefWidth="118.0">
		<font>
			<Font name="D2Coding" size="12.0" />
		</font>
	</TextField>

	<Label alignment="CENTER" layoutX="11.0" layoutY="393.0"
		prefHeight="22.0" prefWidth="50.0" text="생일" textAlignment="RIGHT">
		<font>
			<Font name="D2Coding Bold" size="12.0" />
		</font>
	</Label>
	<TextField fx:id="userBirth_TextField" layoutX="55.0"
		layoutY="393.0" prefHeight="22.0" prefWidth="118.0">
		<font>
			<Font name="D2Coding" size="12.0" />
		</font>
	</TextField>

	<Label alignment="CENTER" layoutX="11.0" layoutY="426.0"
		prefHeight="22.0" prefWidth="50.0" text="성별" textAlignment="RIGHT">
		<font>
			<Font name="D2Coding Bold" size="12.0" />
		</font>
	</Label>
	<TextField fx:id="userGender_TextField" layoutX="55.0"
		layoutY="426.0" prefHeight="22.0" prefWidth="118.0">
		<font>
			<Font name="D2Coding" size="12.0" />
		</font>
	</TextField>

	<Button fx:id="profileEdit_BTN" layoutX="14.0" layoutY="256.0"
		onAction="#profileEdit_Action" prefHeight="22.0" prefWidth="160.0"
